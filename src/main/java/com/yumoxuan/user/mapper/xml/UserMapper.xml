<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yumoxuan.user.mapper.UserMapper">

    <select id="queryUser" resultType="com.yumoxuan.user.entity.User">
        select * from user
    </select>
    <select id="queryById" resultType="com.yumoxuan.user.entity.User" parameterType="java.lang.String">
        select * from user where id=#{id}
    </select>
</mapper>
